<% include partials/header %>

<main id="profile">
    <h1 class="lg-heading">
        <%= user.username %>
    </h1>

    <% therapies.forEach(function(therapy) { %>
        <p><%= therapy.companyName %></p>
    <% }) %>

    <% if (!user.isPro) { %>
        <a href="/basic-setup">Basic Set Up</a>
    <% } else { %>
        <a href="/pro-setup">Pro Setup</a>
    <% } %>
    
</main>

<% include partials/footer %>